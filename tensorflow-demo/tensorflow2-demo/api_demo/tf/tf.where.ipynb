   "source": [
    "import tensorflow as tf\n",
    "\n",
    "\n",
    "labels=tf.constant([0,0,1,1],dtype=tf.float32)\n",
    "one = tf.constant(4,shape=labels.shape,dtype=tf.float32)\n",
    "zero = tf.zeros_like(labels)\n",
    "\n",
    "label = tf.where(tf.less(labels,0.5), x=zero, y=one)\n",
    "\n",
    "label\n"
   ]
